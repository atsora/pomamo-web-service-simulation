{
    "env": {
        "commonjs": true,
        "es6": true,
        "jquery": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "d3": true,
        "exports": true,
        "getPulseTagConfig": true,
        "jqXHR": true,
        "managerConfig": true,
        "moment": true,
        "operatorConfig": true,
        "tagConfig": true,
        "translation": true,
        "xtag": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "indent": ["warn", 2, {"SwitchCase": 1}],
        "quotes": ["warn", "single", { "avoidEscape": true }],
        "keyword-spacing": "warn",
        "brace-style": ["warn", "1tbs", { "allowSingleLine": true }],"no-console": [
            "error",
            {
                "allow": [
                    "assert"
                ]
            }
        ],
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": [
            "error",
            {
                "vars": "all",
                "args": "none"
            }
        ],
        "constructor-super": "warn",
        "valid-typeof": "warn"
    }
}